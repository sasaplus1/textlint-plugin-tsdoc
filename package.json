{
  "name": "textlint-plugin-tsdoc",
  "description": "textlint plugin for tsdoc",
  "version": "0.1.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "dependencies": {
    "@microsoft/tsdoc": "^0.15.0",
    "@textlint/ast-node-types": "^12.2.2",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@textlint/ast-tester": "13.4.1",
    "@textlint/types": "13.4.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.16.2",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "tsup": "8.5.0"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "!dist/*.test.*"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/textlint-plugin-tsdoc.git"
  },
  "scripts": {
    "build": "tsup",
    "fix": "run-p -l fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:fixpack": "npx fixpack",
    "fix:prettier": "prettier --write .",
    "lint": "run-p -l lint:*",
    "lint:eslint": "eslint .",
    "lint:fixpack": "npx fixpack --dryRun",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "prepare": "tsup",
    "pretest": "npm run build",
    "test": "node --test"
  },
  "types": "./dist/index.d.ts"
}
